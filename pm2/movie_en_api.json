{
    "apps": [
        {
            "namespace": "movie_en",
            "name": "api-movie_en",
            "script": "index.js",
            "instances": "max",
            "exec_mode": "cluster",
            "wait_ready": true,
            "listen_timeout": 2000,
            "instance_var": "INSTANCE_ID",
            "log_date_format": "DD-MM-YYYY HH:mm Z",
            "watch": false,
            "env": {
                "APP": "movie_en",
                "SERVICES": ["api"]
            }
        }
    ]
}
