{
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["."],
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-await-in-loop": "off",
        "func-names": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "import/newline-after-import": "off",
        "camelcase": [
            2,
            {
                "properties": "never"
            }
        ],
        "import/order": "off"
    }
}
